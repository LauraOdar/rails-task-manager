<h1>New Task</h1>

<%= form_with model: @task do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :details %>
    <%= f.text_area :details %>
  </div>
    <div>
    <%= f.submit %>
  </div>
  <%=link_to "Back to Task List", tasks_path%>
<% end %>
